  "queryPatterns": {},
  "inferenceRules": [
    {
      "name": "meeting_about_project_from_participants",
      "condition": "MATCH (m:Meeting)<-[:ATTENDS]-(p:Person)-[:WORKS_ON]->(proj:Project)",
      "inference": "MERGE (m)-[:ABOUT_PROJECT]->(proj)",
      "description": "Meeting is about a project if participants work on it"
    },
    {
      "name": "project_belongs_to_company_from_documents",
      "condition": "MATCH (doc:Document)-[:BELONGS_TO_PROJECT]->(proj:Project), (doc)-[:MENTIONS]->(c:Company)",
      "inference": "MERGE (proj)-[:BELONGS_TO_COMPANY]->(c)",
      "description": "Project belongs to a company if its documents mention that company"
    },
    {
      "name": "person_works_on_project_from_tasks",
      "condition": "MATCH (t:Task)-[:ASSIGNED_TO]->(p:Person), (t)-[:BELONGS_TO_PROJECT]->(proj:Project)",
      "inference": "MERGE (p)-[:WORKS_ON]->(proj)",
      "description": "Person works on a project if they have tasks assigned in that project"
    },
    {
      "name": "contact_belongs_to_company_from_email",
      "condition": "MATCH (e:Email)-[:MENTIONS]->(c:Contact), (e)-[:MENTIONS]->(comp:Company)",
      "inference": "MERGE (c)-[:BELONGS_TO_COMPANY]->(comp)",
      "description": "Contact belongs to a company if they appear in the same emails"
    },
    {
      "name": "risk_related_to_project",
      "condition": "MATCH (r:Risk)-[:EXTRACTED_FROM]->(d:Document)-[:BELONGS_TO_PROJECT]->(proj:Project)",
      "inference": "MERGE (r)-[:BELONGS_TO_PROJECT]->(proj)",
      "description": "Risk belongs to a project if extracted from a project document"
    },
    {
      "name": "decision_about_project",
      "condition": "MATCH (d:Decision)-[:EXTRACTED_FROM]->(doc:Document)-[:BELONGS_TO_PROJECT]->(proj:Project)",
